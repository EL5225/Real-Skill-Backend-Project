{
  "name": "real-skill",
  "version": "1.0.0",
  "description": "FInal project BEJS 1 - FEJS 2 Binar Academy",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon app.js",
    "start": "node app.js",
    "migrate": "prisma migrate dev",
    "db-pull": "prisma db pull",
    "postinstall": "prisma generate",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "pretty": "prettier --write .",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn prettier --write",
      "yarn eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^128.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "swagger-ui-express": "^4.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "prettier": "3.1.0",
    "prisma": "^5.6.0"
  }
}
